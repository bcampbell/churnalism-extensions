{
  "manifest_version": 2,

  "name": "Churnalism UK",
  "description": "Highlights press release material in news articles (for the UK).",
  "version": "0.2",
  "icons": { "48": "icon.png" },
  "permissions": [
    "http://*/",
    "https://*/",
    "tabs",
    "webNavigation"
  ],
  "background": {
    "scripts": ["lib/underscore.js","lib/underscore.string.js","lib/moment.min.js","lib/parseuri.js","compat.js","gatso.js","news_sites.js","match.js","tabstate.js","background.js"]
  },
  "browser_action": {
    "default_icon": "icon-off.png",
    "default_popup": "panel.html"
  }
}
